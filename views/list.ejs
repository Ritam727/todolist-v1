<%- include("header"); -%> 
<div class="box" id="heading">
    <h1><%= day %></h1>
</div>
<div class="box">
    <% for(var i = 0; i < listItems.length; i++) { %>
        <div class="item">
            <input type="checkbox"><p><%= listItems[i] %></p>
        </div>
        
    <% } %>
    <form action="/" method="post">
        <input type="text" name="item" placeholder="New Item" autocomplete="off">
        <button type="submit" name="submit" value=<%= day %>>+</button>
    </form>
</div>
<%- include("footer"); -%>